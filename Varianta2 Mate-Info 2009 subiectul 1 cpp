citeste x
daca x>0 atunci 
repeta
 cum este si in eneut 
pana cand x<=0
d)

#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main()
{	
	int x, y;
	cin >> x;
	while (x > 0) {
		cin >> y;
		if (x > y) {
			cout << x % 10;
		}
		else cout << y % 10;
		x = y;
	}
}
