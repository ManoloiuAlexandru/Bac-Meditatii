1. c) 2 (1,3)
2. d) 2^6 nr. de perechi:Combinari de nr.noduri/2 orientat:3(sunt legata de la A la B/nu sunt/sunt legate de la B la A neorientat:2 (sunt/nu sunt legate)
3. C.x = (A.x + B.x) / 2;
   C.y = (A.y + B.y) / 2;
4. Frunze:3,1,2,8 inaltime maxima:3
5. #include <iostream>
#include<string>
using namespace std;

int main()
{
	int n, m;
	cin >> n;
	cin >> m;
	int matr[11][11];
	for (int i = 1;i <= n;i++) {
		for (int j = 1;j <= m;j++)
			if (i <= j) {
				matr[i][j] = i;
			}
			else {
				matr[i][j] = j;
			}
	}
	for (int i = 1;i <= n;i++) {
		for (int j = 1;j <= m;j++) {
			cout << matr[i][j] << " ";
		}
		cout << endl;
	}
}
