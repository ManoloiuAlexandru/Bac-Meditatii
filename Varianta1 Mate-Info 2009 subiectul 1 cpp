2.a) 963
  b) 61,65,67
  c) #include <iostream>
#include<string>
using namespace std;
int main()
{
	int c, n, z = 0, p = 1, cn;
	cin >> n;
	cn = n;
	for (int i = 0;i < to_string(n).length();i++)
	{
		c = cn % 10;
		cn = cn / 10;
		if (c % 3 == 0) {
			z += p * (9 - c);
			p *= 10;
		}
	}
	cout << z;
}
d)#include <iostream>
#include<string>
using namespace std;
int main()
{
	int c, n, z = 0, p = 1;
	cin >> n;
	while (n > 0) {
		c = n % 10;
		n = n / 10;
		if (c % 3 == 0) {
			z += p * (9 - c);
			p *= 10;
		}
	}
	cout << z;
}
